<nav class="d-flex justify-content-center" aria-label="Blog post navigtion">
<ul class="pagination">
    {% if pagination.prev %}
    <li class="page-item">
        <a href="{{pagination.prev.link}}" class="page-link {{pagination.prev.link|length ? '' : 'invisible'}}">Previous</a>
    </li>
    {% endif %}
    {% for page in pagination.pages %}
   		{% if page.link %}
		<li class="page-item">
            <a href="{{page.link}}"  class="page-link">{{page.title}}</a>
		</li>
		{% else %}
		<li class="page-item disabled">
            <a href="#"  class="page-link">{{page.title}}</a>
		</li>
        {% endif %}
    {% endfor %}
    {% if pagination.next %}
    <li class="page-item">
        <a href="{{pagination.next.link}}" class="page-link {{pagination.next.link|length ? '' : 'invisible'}}">Next</a>
    </li>
    {% endif %}
</ul>
</nav>
